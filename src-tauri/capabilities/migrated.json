{
  "identifier": "migrated",
  "description": "permissions that were migrated from v1",
  "local": true,
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    {
      "identifier": "fs:scope",
      "allow": [
        "**",
        "**/*",
        "/**/*"
      ]
    },
    "shell:allow-open",
    "dialog:allow-open",
    {
      "identifier": "http:default",
      "allow": [
        {
          "url": "https://www.last.fm/*"
        },
        {
          "url": "https://api.deezer.com/*"
        },
        {
          "url": "https://e-cdns-images.dzcdn.net/images/cover/*"
        }
      ]
    },
    "http:default",
    "dialog:default",
    "shell:default",
    "fs:default"
  ]
}