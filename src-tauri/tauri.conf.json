{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "frontendDist": "../dist",
    "devUrl": "http://localhost:1420"
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "db/schema.sql"
    ]
  },
  "productName": "sable",
  "version": "0.1.0",
  "identifier": "com.errormine.sable",
  "plugins": {},
  "app": {
    "security": {
      "assetProtocol": {
        "scope": [
          "**"
        ],
        "enable": true
      },
      "csp": null
    },
    "windows": [
      {
        "title": "Sable",
        "width": 1400,
        "height": 800
      }
    ]
  }
}